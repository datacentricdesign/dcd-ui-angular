{
  "$schema": "../../node_modules/ng-packagr/ng-package.schema.json",
  "dest": "../../dist/ui-angular",
  "lib": {
    "entryFile": "src/public-api.ts",
    "umdModuleIds": {
      "primeng/slider" : "primengSlider",
      "primeng/dialog" : "primengDialog",
      "primeng/calendar": "primengCalendar",
      "primeng/checkbox" : "primengCheckbox",
      "primeng/inputtext" : "primengInputText",
      "@ng-bootstrap/ng-bootstrap" : "ngBootstrap",
      "ng2-charts" : "ng2Charts",
      "@swimlane/ngx-charts":"ngxCharts",
      "@swimlane/ngx-charts/release/utils":"ngxChartsReleaseUtils",
      "ngx-clipboard": "ngxClipboard",
      "d3-scale":"d3Scale",
      "d3-shape":"d3shape",
      "web-google-maps":"webGoogleMaps",
      "angular-vertical-timeline":"verticalTimeline"
    }
  },
  "whitelistedNonPeerDependencies": [
    "chart.js",
    "ng2-charts",
    "primeicons",
    "primeng",
    "@swimlane/ngx-charts",
    "ngx-clipboard",
    "d3-scale",
    "d3-shape",
    "web-google-maps",
    "@fortawesome/fontawesome-free",
    "bootstrap",
    "@ng-bootstrap/ng-bootstrap",
    "angular-vertical-timeline",
    "d3-timelines"
  ]
}
